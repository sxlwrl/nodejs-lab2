{
  "name": "nodejs-lab2",
  "version": "1.0.0",
  "description": "second lab",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npx babel src --out-dir dist --extensions \".ts\" --source-maps inline\n",
    "start": "node dist/server.js",
    "dev": "ts-node --esm ./src/server.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "npx prettier src/ --check",
    "format:fix": "npx prettier src/ --write",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxlwrl/nodejs-lab2.git"
  },
  "author": "sxlwrl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sxlwrl/nodejs-lab2/issues"
  },
  "homepage": "https://github.com/sxlwrl/nodejs-lab2#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "babel-plugin-add-import-extension": "^1.6.0",
    "eslint": "^8.56.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sonarjs": "^0.23.0",
    "husky": "^8.0.3",
    "prettier": "3.1.1",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "typescript": "^5.3.3"
  }
}
